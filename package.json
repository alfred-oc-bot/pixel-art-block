{
  "name": "pixel-art-block",
  "version": "1.0.0",
  "description": "A WordPress Gutenberg block for creating pixel art",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "test": "jest",
    "test:schema": "jest --testPathPattern=blueprint",
    "test:execution": "jest --testPathPattern=execution",
    "test:all": "jest --testPathPattern='(blueprint|execution)'",
    "validate:blueprint": "ajv validate -s https://playground.wordpress.net/blueprint-schema.json -d .blueprint.json"
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "block",
    "pixel-art"
  ],
  "author": "",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@wordpress/scripts": "^26.0.0",
    "@wp-playground/cli": "^3.0.53",
    "ajv": "^8.18.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@wordpress/block-editor": "^13.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^3.0.0",
    "@wordpress/i18n": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 60000,
    "verbose": true
  }
}
